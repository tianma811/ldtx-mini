.page-section-gap{
  box-sizing: border-box;
  padding: 0 30rpx;
}

.page-body-button {
  margin-bottom: 30rpx;
}

.qrcode{
  position:absolute;
  width:460rpx;
  height:460rpx;
  top:calc(50% - 290rpx);
  left:calc(50% - 230rpx );
  border-radius:50%;
}
.shareBg{
  position: absolute;
  z-index: -11;
}
.shareBox{
  background:rgba(255,255,255,0.9);
  margin:0rpx 50rpx;
  padding:20rpx;
  border-radius:30rpx;
  height:900rpx;
  position:absolute;
  width:600rpx;
}
.shareBox .houseImg{
  width:100%;height:30vh;
}
.shareInfo{
  line-height: 30rpx;
  margin-top: 30rpx;
}
.shareInfo view{
  width: 30%;
  display: inline-block;
  font-size:25rpx;
}
.shareInfo view text{
  display:block;
  color: #009241;
  font-size:30rpx;
  line-height: 100rpx;
}
.title{
  font-size: 35rpx;
  line-height:80rpx;
}
.shareImg{ float: right;} 
.shareImg image{width:25vh;height:25vh;border-radius:50%;}
.jingjirenPhone{
  line-height: 100rpx;
  height: 100rpx;
  margin-top:100rpx;
  text-align:center;
  font-size:35rpx;
}
.jingjirenPhone text {
  color: #009241;
}
.shareLogo{
  width: 120rpx;
  height: 120rpx;
  margin: 50rpx;
}
.shareTitle{
  font-size:40rpx;
  font-weight:bold;
  position:absolute;
  margin-left:200rpx;
  margin-top:80rpx;
  color:#000;
}
.shareTitle text{
  font-weight:normal;
  
}